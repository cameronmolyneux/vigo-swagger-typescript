    "eslint": "^7.8.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.3",
    "jest": "^27.0.4",
    "prettier": "^2.2.1",
    "prettier-plugin-jsdoc": "^0.3.13",
    "standard-version": "^9.0.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
